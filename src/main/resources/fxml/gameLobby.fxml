<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane id="background" stylesheets="@/CSS/style.css" xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml">
    <center>
        <AnchorPane maxHeight="375.0" maxWidth="325.0" prefHeight="375.0" prefWidth="325.0"
                    BorderPane.alignment="CENTER">
            <children>
                <AnchorPane id="backgroundCheckbox" layoutX="13.0" layoutY="8.0" prefHeight="40.0" prefWidth="96.0"/>
                <CheckBox id="isAIBox" fx:id="isAIBox" selected="true" layoutX="24.0" layoutY="15.0"
                          mnemonicParsing="false" prefHeight="25.0" prefWidth="81.0" text="Play as AI"/>
                <ListView fx:id="challengerList" layoutX="13.0" layoutY="61.0" prefHeight="143.0" prefWidth="300.0"/>
                <Button id="button" layoutX="211.0" layoutY="8.0" onAction="#pressRefresh" prefHeight="40.0"
                        prefWidth="100.0" text="REFRESH"/>
                <Button id="button" layoutX="13.0" layoutY="217.0" onAction="#pressSubscribe" prefHeight="40.0"
                        prefWidth="300.0" text="SUBSCRIBE"/>
                <Button id="button" layoutX="13.0" layoutY="269.0" onAction="#pressChallengePlayer" prefHeight="40.0"
                        prefWidth="300.0" text="CHALLENGE PLAYER"/>
                <Button id="button" layoutX="13.0" layoutY="321.0" onAction="#pressBackToMainMenu" prefHeight="40.0"
                        prefWidth="300.0" text="MAIN MENU"/>
                <Label fx:id="messageLabel" layoutX="13.0" layoutY="372.0" prefHeight="17.0" prefWidth="300.0"/>
            </children>
        </AnchorPane>
    </center>
</BorderPane>
